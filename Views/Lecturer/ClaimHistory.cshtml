@model IEnumerable<ST10298613_PROG6212_POE.Models.Claim>

<h1>Claim History</h1>
<table class="table">
    <thead>
        <tr>
            <th>Claim ID</th>
            <th>Hours Worked</th>
            <th>Hourly Rate</th>
            <th>Total Payment</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.Id</td>
                <td>@claim.HoursWorked</td>
                <td>@claim.HourlyRate</td>
                <td>@claim.TotalPayment</td>
                <td>Approved</td> <!-- Replace with actual status -->
            </tr>
        }
    </tbody>
</table>